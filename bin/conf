contents="dummy_contents"	# 記事リポジトリの名前
contents_owner="kiyotosi"		# 記事リポジトリの持ち主のユーザ名
wwwdir="/var/www"				# 記事リポジトリやbashcms2を置くディレクトリ
contentsdir="$wwwdir/$contents"	# 記事リポジトリのパス
appdir="$wwwdir/bashcms2"		# bashcms2をインストールするパス
viewdir="$appdir/view"			# 
logdir="/var/log/$contents"		# 
datadir="${contentsdir}_data"	# データディレクトリ(タイムスタンプ格納用)
export LANG=ja_JP.UTF-8
